package ua.nure.zharkov.Task1;

public class Part6 {

	public static int[] FibbNums(int n) {
		int[] result = new int[n];
		if (n == 1) {
			result[0] = 1;
		} else {
			result[1] = (result[0] = 1);
			for (int i = 2; i < n; ++i) {
				result[i] = result[i - 2] + result[i - 1];
			}
		}
		return result;
	}

	public static String StrArray(int[] arr) {
		String out = new String();
		for (int i = 0; i < arr.length; ++i) {
			out += (i == (arr.length - 1)) ? arr[i] : (arr[i] + ", ");
		}
		return out;
	}

	public static void main(String[] args) {
		if (args.length == 0) {
			System.out.print("Function got no parameters");
		} else {
			int[] outData = FibbNums(Integer.parseInt(args[0]));
			System.out.print("=== Part6 \n" + StrArray(outData) + "\n");
		}
	}

}
